{
  "name": "lines-svg",
  "version": "0.3.0",
  "description": "LinesSvg is a financial chart library based on snapsvg",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bbvox/lines-svg.git"
  },
  "config": {
    "svgSrc": "node_modules/snapsvg/dist/snap.svg-min.js",
    "src": "src/lines.js",
    "dest": "dist/lines.js",
    "timeout": 10000
  },
  "scripts": {
    "start": "serve ./",
    "build:watch": "rollup -f iife -i lib/index.js -o dist/build.js -w",
    "test": "mocha --recursive --timeout $npm_package_config_timeout --exit",
    "test1": "mocha test/index.test.js --recursive --timeout $npm_package_config_timeout --exit",
    "test:cover": "istanbul cover _mocha test/*.js",
    "build": "npm run build1 && npm run build2 && npm run build3",
    "build1": "rm -rf dist .tmp",
    "build2": "mkdir .tmp dist",
    "build3": "cat $npm_package_config_svgSrc $npm_package_config_src > $npm_package_config_dest",
    "lint": "eslint .",
    "cover": "istanbul cover _mocha test/*.js",
    "lcov": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build:up": "rollup -f iife -i lib/index.js -o dist/build.js",
    "build2:up": "rollup -f iife -i tindex.js -o b.js",
    "build2:watch": "rollup -f iife -i tsc/index.js -o tsc/b.js -w",
    "bld3": "rollup -f iife --globals jquery:jQuery,lodash:_ -i src/app.js -o build/app.js -m build/app.js.map"
  },
  "keywords": [
    "svg",
    "forex charts",
    "candlestick",
    "averages",
    "responsive"
  ],
  "author": "bbvox",
  "license": "MIT",
  "dependencies": {
    "snapsvg": "*"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.5.0",
    "istanbul": "^0.4.5",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.2.1",
    "sinon": "^7.5.0",
    "rollup": "^1.23.1",
    "serve": "^11.2.0"
  }
}